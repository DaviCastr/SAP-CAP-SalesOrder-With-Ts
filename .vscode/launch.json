{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug App Sales Order",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "cds-tsx",
        "watch"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "develoment"
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      
    }
  ]
}
